{
	"name": "sveltekit-forms",
	"version": "1.0.61",
	"description": "Sveltekit Form Actions Wrapper",
	"author": "fabriziotognetto <info@quantos.it>",
	"repository": "github:ftognetto/sveltekit-forms",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package -o package",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "npm run build && npm publish"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.15.0",
		"svelte": "^3.58.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.32.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.0",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"form-data": "4.0.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.1.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.2.1",
		"vitest": "^0.29.2",
		"zod": "^3.21.4"
	},
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./components/form.svelte": {
			"types": "./package/components/form.svelte.d.ts",
			"svelte": "./package/components/form.svelte",
			"default": "./package/components/form.svelte"
		},
		"./components/form_checkbox.svelte": {
			"types": "./package/components/form_checkbox.svelte.d.ts",
			"svelte": "./package/components/form_checkbox.svelte",
			"default": "./package/components/form_checkbox.svelte"
		},
		"./components/form_input.svelte": {
			"types": "./package/components/form_input.svelte.d.ts",
			"svelte": "./package/components/form_input.svelte",
			"default": "./package/components/form_input.svelte"
		},
		"./components/form_radio.svelte": {
			"types": "./package/components/form_radio.svelte.d.ts",
			"svelte": "./package/components/form_radio.svelte",
			"default": "./package/components/form_radio.svelte"
		},
		"./components/form_select.svelte": {
			"types": "./package/components/form_select.svelte.d.ts",
			"svelte": "./package/components/form_select.svelte",
			"default": "./package/components/form_select.svelte"
		},
		"./components/form_textarea.svelte": {
			"types": "./package/components/form_textarea.svelte.d.ts",
			"svelte": "./package/components/form_textarea.svelte",
			"default": "./package/components/form_textarea.svelte"
		},
		"./helpers/form_decoder": {
			"types": "./package/helpers/form_decoder.d.ts",
			"default": "./package/helpers/form_decoder.js"
		},
		"./helpers/form_validator": {
			"types": "./package/helpers/form_validator.d.ts",
			"default": "./package/helpers/form_validator.js"
		},
		"./helpers/zod_validator_helper": {
			"types": "./package/helpers/zod_validator_helper.d.ts",
			"default": "./package/helpers/zod_validator_helper.js"
		},
		".": {
			"types": "./package/index.d.ts",
			"svelte": "./package/index.js",
			"default": "./package/index.js"
		}
	},
	"files": [
		"package"
	],
	"svelte": "./package/index.js",
	"typesVersions": {
		">4.0": {
			"components/form.svelte": [
				"./package/components/form.svelte.d.ts"
			],
			"components/form_checkbox.svelte": [
				"./package/components/form_checkbox.svelte.d.ts"
			],
			"components/form_input.svelte": [
				"./package/components/form_input.svelte.d.ts"
			],
			"components/form_radio.svelte": [
				"./package/components/form_radio.svelte.d.ts"
			],
			"components/form_select.svelte": [
				"./package/components/form_select.svelte.d.ts"
			],
			"components/form_textarea.svelte": [
				"./package/components/form_textarea.svelte.d.ts"
			],
			"helpers/form_decoder": [
				"./package/helpers/form_decoder.d.ts"
			],
			"helpers/form_validator": [
				"./package/helpers/form_validator.d.ts"
			],
			"helpers/zod_validator_helper": [
				"./package/helpers/zod_validator_helper.d.ts"
			],
			"index": [
				"./package/index.d.ts"
			]
		}
	}
}
